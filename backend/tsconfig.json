{
    "compilerOptions": {
        "incremental": true,
        "tsBuildInfoFile": ".tsbuildinfo",
        "sourceMap": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "es2017",
        "allowJs": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "removeComments": false,
        "strict": true
    },
}